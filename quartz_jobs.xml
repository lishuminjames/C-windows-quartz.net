<?xml version="1.0" encoding="utf-8" ?>

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>

    <!--TestJob测试 任务配置-->
    <job>
      <name>TestJob</name>
      <group>Test</group>
      <description>TestJob测试</description>
      <job-type>GetBaseDataWindowsService.TestJob,GetBaseDataWindowsService</job-type>
      <!--任务类型：实现ijob的类与命名空间-->
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <!--配置服务触发器-->
    <trigger>
      <cron>
        <name>TestJobTrigger</name>
        <group>Test</group>
        <job-name>TestJob</job-name>
        <job-group>Test</job-group>
        <start-time>2015-11-26T22:19:00+08:00</start-time>
        <cron-expression>0/30 * * * * ?</cron-expression>
        <!-- 秒 分 时 日 月 星期 年（可选） -->
      </cron>
    </trigger>

    <job>
      <name>TestJobtwo</name>
      <group>Test</group>
      <description>TesttwoJob测试</description>
      <job-type>GetBaseDataWindowsService.TestJobtwo,GetBaseDataWindowsService</job-type>
      <!--任务类型：实现ijob的类与命名空间-->
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <!--配置服务触发器-->
    <trigger>
      <cron>
        <name>TestJobtwoTrigger</name>
        <group>Test</group>
        <job-name>TestJobtwo</job-name>
        <job-group>Test</job-group>
        <start-time>2015-11-26T22:19:00+08:00</start-time>
        <cron-expression>0/20 * * * * ?</cron-expression>
        <!-- 秒 分 时 日 月 星期 年（可选） -->
      </cron>
    </trigger>

  </schedule>
</job-scheduling-data>